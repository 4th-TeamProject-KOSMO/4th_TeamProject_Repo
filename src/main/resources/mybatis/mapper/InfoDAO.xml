<?xml version="1.0" encoding="UTF-8"?>


<!-- 컨트롤러가 호출하는 인터페이스의 위치를 namespace로 지정한다. -->
<mapper namespace="com.edu.springboot.info.IBoardService">

	<select id="selectOne"
		parameterType="int"
		resultType="com.edu.springboot.info.BoardDTO">
		SELECT * from info where plantidx = ${plantidx}
	</select>
	
	<select id="selectAll"
		resultType="com.edu.springboot.info.BoardDTO">
		SELECT * from info
	</select>

</mapper>